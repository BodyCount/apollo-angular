{
  "name": "apollo-angular",
  "description": "Use your GraphQL data in your Angular app, with the Apollo Client",
  "version": "2.0.0-beta.1",
  "author": {
    "name": "Kamil Kisiela",
    "email": "kamil.kisiela@gmail.com",
    "url": "https://github.com/kamilkisiela/"
  },
  "license": "MIT",
  "sideEffects": false,
  "main": "build/bundles/ngApollo.umd.js",
  "module": "build/fesm5/ngApollo.js",
  "typings": "build/ngApollo.d.ts",
  "homepage": "https://www.apollographql.com/docs/angular/",
  "repository": {
    "type": "git",
    "url": "https://github.com/apollographql/apollo-angular",
    "directory": "packages/apollo-angular"
  },
  "keywords": [
    "ngx",
    "apollo",
    "graphql",
    "angular",
    "schematics",
    "angular-schematics"
  ],
  "scripts": {
    "build": "ng-packagr -p ng-package.js && yarn build:schematics",
    "build:schematics": "tsc -p schematics/tsconfig.json && node ./scripts/move-schematics.js",
    "test": "yarn test-only",
    "posttest": "yarn test:testing && yarn test:schematics",
    "test-only": "jest --config jest.config.js",
    "test:testing": "jest --config testing/jest.config.js",
    "test:schematics": "jest --config schematics/jest.config.js",
    "prebuild": "rm -rf build/",
    "postbuild": "yarn check:umd",
    "check:umd": "node build/bundles/ngApollo.umd.js",
    "deploy": "yarn build && npm publish build --tag next"
  },
  "peerDependencies": {
    "@angular/common": "^6.0.0 || ^7.0.0 || ^8.0.0 || ^9.0.0 || ^9.0.0-rc.0",
    "@angular/core": "^6.1.0 || ^7.0.0 || ^8.0.0 || ^9.0.0 || ^9.0.0-rc.0",
    "@apollo/client": "^3.0.0-beta.19",
    "rxjs": "^6.0.0"
  },
  "dependencies": {
    "@apollo/link-batch": "2.0.0-beta.3",
    "extract-files": "7.0.0"
  },
  "devDependencies": {
    "@angular-devkit/core": "9.0.0-rc.10",
    "@angular-devkit/schematics": "9.0.0-rc.10",
    "@angular/animations": "9.0.0-rc.11",
    "@angular/common": "9.0.0-rc.11",
    "@angular/compiler": "9.0.0-rc.11",
    "@angular/compiler-cli": "9.0.0-rc.11",
    "@angular/core": "9.0.0-rc.11",
    "@angular/platform-browser": "9.0.0-rc.11",
    "@angular/platform-browser-dynamic": "9.0.0-rc.11",
    "@angular/platform-server": "9.0.0-rc.11",
    "@apollo/client": "3.0.0-beta.33",
    "@schematics/angular": "9.0.0-rc.10",
    "@types/jest": "25.1.1",
    "@types/node": "13.7.0",
    "@types/semver": "7.1.0",
    "cpx": "1.5.0",
    "graphql": "14.6.0",
    "jest": "25.1.0",
    "jest-zone-patch": "0.0.10",
    "ng-packagr": "9.0.0-rc.7",
    "rxjs": "6.5.4",
    "ts-jest": "25.2.0",
    "typescript": "3.7.5",
    "zone.js": "0.10.2"
  },
  "schematics": "./schematics/collection.json",
  "ng-update": {
    "requirements": {},
    "migrations": "./schematics/migrations.json",
    "packageGroupName": "apollo-angular",
    "packageGroup": [
      "apollo-angular",
      "@apollo/client",
      "graphql"
    ]
  }
}
